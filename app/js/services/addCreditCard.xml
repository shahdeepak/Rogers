<CreditCardInfo preferred="$creditcard.Preferred" saveToProfile="$creditcard.SaveToProfile" cvv="$creditcard.CVV" xmlns="http://api.redbox.com/v5/CustomerAccount">
	<Number>$creditcard.Number</Number>
	<FirstName>$creditcard.FirstName</FirstName>
	<LastName>$creditcard.LastName</LastName>
	<Zip>$creditcard.Zip</Zip>
	<Month>$creditcard.ExpirationMonth</Month>
	<Year>$creditcard.ExpirationYear</Year>
	<Alias>$creditcard.Alias</Alias>
	<AccountBillingAddress>
		<CityName>$creditcard.City</CityName>
		<StateCd>$creditcard.State</StateCd>
		<StreetAddressLine1>$creditcard.Address1</StreetAddressLine1>
		<StreetAddressLine2>$creditcard.Address2</StreetAddressLine2>
		<ZipPostalCode>$creditcard.ZipPostalCode</ZipPostalCode>
	</AccountBillingAddress>
</CreditCardInfo>