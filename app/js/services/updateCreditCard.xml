<AccountInfo isPreferredAccount="$creditcard.Preferred" avsChecked="$creditcard.AVSChecked"
	cvvChecked="$creditcard.CVVChecked" cvv="$creditcard.CVV" xmlns="http://api.redbox.com/v5/CustomerAccount">
	<AccountNumber>$creditcard.AccountNumber</AccountNumber>
	<AccountAlias>$creditcard.AccountAlias</AccountAlias>
	<FirstName>$creditcard.FirstName</FirstName>
	<LastName>$creditcard.LastName</LastName>
	<LastFour>$creditcard.LastFour</LastFour>
	<ExpirationMonth>$creditcard.ExpirationMonth</ExpirationMonth>
	<ExpirationYear>$creditcard.ExpirationYear</ExpirationYear>
	<AccountBillingAddress>
		<CityName>$creditcard.City</CityName>
		<StateCd>$creditcard.State</StateCd>
		<StreetAddressLine1>$creditcard.Address1</StreetAddressLine1>
		<StreetAddressLine2>$creditcard.Address2</StreetAddressLine2>
		<ZipPostalCode>$creditcard.ZipPostalCode</ZipPostalCode>
	</AccountBillingAddress>
	<Extensions />
</AccountInfo>